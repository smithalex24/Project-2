<form method = "POST" action = "/auth/signup">
	<div class = "form-group">
		<label>Name</label>
		<input id = "name" type = "text" name = "name" class = "form-control" placeholder = "What's your name?">
	</div>
	<div class = "form-group">
		<label>Email</label>
		<input id = "email" type = "text" name = "email" class = "form-control" placeholder = "Email goes here">
	</div>
	<div class = "form-group">
		<label>Password</label>
		<input id = "password" type = "password" name = "password" class = "form-control" placeholder = "Secret password goes here">
	</div>
	<div class = "form-group">
	<input type = "submit" value = "Sign Up!" class = "btn btn-success" id = "signUpButton2">
</div>
</form>
